{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "zeroConfig": true
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/.*\\.js$",
      "headers": {
        "Content-Type": "text/javascript; charset=utf-8"
      }
    },
    {
      "src": "/assets/.*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)\\.js$",
      "headers": {
        "Content-Type": "text/javascript; charset=utf-8"
      }
    },
    {
      "src": "/(.*)\\.css$",
      "headers": {
        "Content-Type": "text/css; charset=utf-8"
      }
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html",
      "headers": {
        "Content-Type": "text/html; charset=utf-8"
      }
    }
  ]
} 